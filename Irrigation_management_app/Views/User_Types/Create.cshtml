@model Irrigation_management_app.Models.User_TypesVModel

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="User_Type_Name" class="control-label">User Type Name</label>
                <input asp-for="User_Type_Name" class="form-control" />
                <span asp-validation-for="User_Type_Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-primary">Create</button>
                <a asp-action="Index" class="btn btn-secondary">Go back</a>
            </div>
        </form>
    </div>
</div>